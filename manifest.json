{
    "manifest_version": 3,
    "name": "My Extension",
    "version": "1.0",
    "description": "An example Chrome extension with a popup.",
    "permissions": [
    "storage",
    "activeTab",
    "tabs"
    ],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html"
    },
    "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
  }